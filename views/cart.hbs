<h1>My Cart</h1>

{{#if empty}}
    <h2>Your cart is empty.</h2>
{{else}}
    <h2>There are {{ count }} in your cart.</h2>

    {{#each myCart}}
        <div>
            {{ this.name }}, {{ this.quantity }}
        </div>
    {{/each}}
{{/if}} 

